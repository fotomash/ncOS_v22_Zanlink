{
  "version": "v23.0.0",
  "system_name": "ncOS Ultimate Trading System",
  "description": "Unified vector-native framework with ZanFlow integration",
  "vector_config": {
    "dimension": 1536,
    "similarity_threshold": 0.85,
    "max_vectors_per_session": 10000
  },
  "agents": {
    "smc_master": {
      "class": "SMCMasterAgent",
      "weight": 1.5,
      "config": {
        "structure_lookback": 50,
        "fvg_min_size": 0.5
      }
    },
    "liquidity_sniper": {
      "class": "LiquiditySniperAgent",
      "weight": 1.2,
      "config": {
        "sweep_sensitivity": "high",
        "trap_detection": "aggressive"
      }
    },
    "risk_guardian": {
      "class": "RiskGuardianAgent",
      "weight": 2.0,
      "config": {
        "max_risk": 0.02,
        "position_model": "kelly"
      }
    }
  },
  "strategies": {
    "prometheus": {
      "min_similarity": 0.85,
      "lookback_patterns": 1000,
      "confidence_threshold": 0.8
    },
    "wyckoff": {
      "phase_detection": true,
      "volume_analysis": true,
      "spring_sensitivity": 0.002
    },
    "maz": {
      "unmitigated_only": true,
      "min_distance": 0.002,
      "validity_hours": 168
    }
  },
  "data_config": {
    "external_path": "/data",
    "supported_formats": [
      "csv",
      "json",
      "parquet"
    ],
    "enrichment_indicators": [
      "atr",
      "rsi",
      "macd",
      "volume_profile"
    ],
    "tick_aggregation": {
      "M1": 60,
      "M5": 300,
      "M15": 900,
      "H1": 3600
    }
  },
  "risk_management": {
    "max_risk_per_trade": 0.02,
    "max_daily_drawdown": 0.06,
    "max_positions": 3,
    "position_sizing": "dynamic",
    "stop_loss_mode": "atr_based"
  },
  "confluence_scoring": {
    "min_factors": 3,
    "factor_weights": {
      "structure": 1.5,
      "liquidity": 1.2,
      "poi": 1.3,
      "volume": 1.0,
      "time": 0.8
    }
  },
  "menu_config": {
    "auto_display": true,
    "navigation_mode": "numeric",
    "quick_actions": [
      "scan",
      "signals",
      "analysis",
      "risk"
    ]
  }
}